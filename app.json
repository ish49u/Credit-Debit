{
  "expo": {
    "name": "Credit-Debit",
    "slug": "creditdebitapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "creditdebitapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.creditdebit.creditdebitapp"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicons.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#ffffff"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true,
      "navigationPersistence": false 
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "35b6090d-1020-4a49-9a67-c189bd015729"
      }
    }
  }
}
